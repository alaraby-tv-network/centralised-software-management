<div class="page-header">
  <%= link_to end_users_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_end_user_path(@end_user), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show end_user</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @end_user.name %></dd>

  <dt>Email:</dt>
  <dd><%= @end_user.email %></dd>

</dl>
